{
  "name": "react-dropdown-select",
  "version": "4.9.3",
  "description": "Customizable dropdown select for react",
  "main": "dist/react-dropdown-select.js",
  "module": "lib/index.js",
  "types": "types.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "lib",
    "types.d.ts"
  ],
  "scripts": {
    "test": "jest --bail",
    "test:watch": "jest --bail --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "build": "webpack && npm run transpile",
    "transpile": "babel src -d lib",
    "prepublishOnly": "npm run build",
    "version": "replace \"vPLACEHOLDER\" \"v$npm_package_version\" ./CHANGELOG.md && git add .",
    "preversion": "sh change-log-builder.sh",
    "release:patch": "npm version patch -m \"v%s \nSee changelog: https://github.com/sanusart/react-dropdown-select/blob/master/CHANGELOG.md\"",
    "release:minor": "npm version minor -m \"v%s \nSee changelog: https://github.com/sanusart/react-dropdown-select/blob/master/CHANGELOG.md\"",
    "release:major": "npm version major -m \"v%s \nSee changelog: https://github.com/sanusart/react-dropdown-select/blob/master/CHANGELOG.md\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sanusart/react-dropdown-select"
  },
  "bundlesize": [
    {
      "path": "./dist/react-dropdown-select.js",
      "maxSize": "20 kB"
    },
    {
      "path": "./lib/index.js",
      "maxSize": "20 kB"
    }
  ],
  "keywords": [
    "react",
    "react component",
    "select",
    "multiselect",
    "dropdown"
  ],
  "author": "Sasha Khamkov <sanusart@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "prop-types": ">=15.7.2",
    "react": ">=16.x",
    "react-dom": ">=16.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@emotion/babel-plugin": "11.7.2",
    "@emotion/jest": "^11.8.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.27",
    "babel-plugin-transform-dynamic-import": "^2.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-minify": "^0.5.1",
    "bundlesize": "^0.18.0",
    "coveralls": "^3.0.9",
    "css-loader": "^6.7.3",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^3.1.0",
    "jest": "^25.3.0",
    "prettier": "1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "replace": "^1.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@emotion/react": "11.8.1",
    "@emotion/styled": "11.8.1"
  },
  "jest": {
    "snapshotSerializers": [
      "@emotion/jest/serializer"
    ],
    "transform": {
      "^.+\\.(js)$": "babel-jest"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).js?(x)"
    ],
    "testURL": "http://localhost/",
    "collectCoverage": false,
    "bail": true,
    "roots": [
      "<rootDir>/__tests__"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/ClickOutside.js",
      "!node_modules/**",
      "!docs/**",
      "!dist/**",
      "!coverage/**"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "ie 11"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
